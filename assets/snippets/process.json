[
    {
        "Line": "PROCESS {\n"
    },

    {
        "Line": "$result = (Resolve-DnsName -Name $ComputerName).IPAddress\n"
    },

    {
        "Line": "$result\n"
    },
    {
        "Line": "$totalCount++\n"
    },
    {
        "Line": "$objComputer  = New-Object psobject -Property @{ComputerName = $computerName;IP = $result}\n"
    },
    {
        "Line": "$collectionTable += $objComputer\n"
    },
    {
        "Line": "}\n"
    }
]